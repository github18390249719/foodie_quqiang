<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="main_menu_tv_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/main_text_bg</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:textColor">@drawable/main_text_color</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="frl_menu_ll_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="frl_best_seller_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="frl_homepage_menu_iv_style">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">80dp</item>
    </style>

    <style name="frl_homepage_menu_tv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:textColor">#5E3D50</item>
    </style>

    <style name="frl_menu_ll_tv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">16dp</item>
    </style>

    <style name="frl_ranking_tv_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@drawable/frl_ranking_text</item>
        <item name="android:textSize">12dp</item>
    </style>
    <style name="homepage">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">18sp</item>
        <item name="android:shadowColor">@color/red</item>
        <item name="android:shadowRadius">3.0</item>
        <item name="android:shadowDx">3.0</item>
        <item name="android:shadowDy">3.0</item>
    </style>
    <style name="register_class_ll_style">
        <item name="android:background">@color/snow</item>
        <item name="android:layout_width">300dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="register_class_tv_style">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/dimgray</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="register_class_et_style">
        <item name="android:layout_width">220dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textColorHint">#CDCDC1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/limegreen</item>
        <item name="android:background">@null</item>
    </style>
    <style name="setting_textView">
        <item name="android:drawableRight">@mipmap/u917</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:background">#33CCCCCC</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center_vertical|left</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="homepage_menu_ll_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="homepage_vp_cursor_ll">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">10dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="homepage_food_recommend_ll_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="homepage_food_recommend_iv_style">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">80dp</item>
    </style>
    <style name="homepage_food_recommend_tv_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="homepage_recommend_class_tv_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#3F2B36</item>
    </style>
    <style name="homepage_menu_iv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">40dp</item>
    </style>
    <style name="homepage_menu_tv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:textSize">16dp</item>
    </style>
    <style name="food_search_content_class_tv_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:textColor">#592C63</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="food_search_content_tv">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:background">@color/ghostwhite</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="food_search_content_ll_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/shape2</item>
    </style>
</resources>
